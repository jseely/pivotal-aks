credhub:
  database: mysql
  spring:
    datasource:
      username: "${CREDHUB_DATASOURCE_USERNAME}"
      password: "${CREDHUB_DATASOURCE_PASSWORD}"
      url: "${CREDHUB_DATASOURCE_URL}"
  authServer:
    # This should probably be something like uaa.default.svc.cluster.local
    url: "${CREDHUB_AUTH_SERVER_URL}"
  authServerCa: "${CREDHUB_AUTH_SERVER_CA}"
